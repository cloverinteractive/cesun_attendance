<!DOCTYPE html>
<html>
  <head>
    <title>CESUN Asistencia</title>
    <%= stylesheet_link_tag 'application', 'token_input/token-input-facebook' %>
    <%= javascript_include_tag :defaults, 'token_input/jquery.tokeninput' %>
    <%= csrf_meta_tag %>
    <%= yield :head %>
    <meta charset="utf-8">
  </head>
  <body>
    <div id="heading">
      <%= render 'layouts/account_info' %>
    </div>
    <% if devise_controller? %>
      <%= yield %>
    <% else %>
      <div id="wrapper">
        <div id="page_content">
          <%= yield %>
        </div>
      </div>
    <% end %>
  </body>
</html>
